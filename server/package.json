{
  "name": "react-express-auth-template",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "knex": "knex",
    "build": "npm i && npm run migrate:rollback && npm run migrate && npm run seed",
    "start": "concurrently \"npm run start:react\" \"npm run start:server\" \"npm run start:flask\"",
    "dev": "nodemon ./index.ts",
    "connect": "psql -U postgres -d react_auth_example",
    "migrate": "npx knex migrate:latest",
    "migrate:make": " npx knex migrate:make",
    "migrate:rollback": "npx knex migrate:rollback",
    "seed": "npx knex seed:run",
    "seed:make": "knex seed:make",
    "lint": "eslint . --fix",
    "prebuild": "tsc",
    "prestart": "tsc",
    "predev": "tsc",
    "start:react": "react-scripts start",
    "start:server": "tsc && node dist/server.js",
    "start:flask": "cd backend && source venv/bin/activate && python3 app.py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/aos": "^3.0.7",
    "bcrypt": "^5.1.0",
    "concurrently": "^8.2.2",
    "connect-session-knex": "^3.0.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "knex": "^2.4.2",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2"
  },
  "description": ""
}
